pipeline {
agent any

stages {
stage('checkout') {
steps {
git branch:'main',url:'https://github.com/sravanthikhatha/gitproject/new/main'
}
}

stage('Deploy'){
steps{
sh"
mkdir -p deployed_html
cp * .html deployed_html/
' ' '
}
}

post {
success {
publishHTML(target: [
allowMissing: false,
keepAll: true,
reportDir: 'deployed_html',
reportFiles: 'form.html',
reportName: 'My Webpage' 
]) 
}
}
}

